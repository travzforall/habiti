{
  "table_name": "habit_templates",
  "description": "Pre-defined habit templates for easy habit creation - Import SIXTH",
  "import_order": 6,
  "dependencies": ["habit_categories", "habit_subcategories"],
  "fields": {
    "id": {"type": "text", "primary": true},
    "name": {"type": "text", "required": true},
    "category_id": {"type": "link", "link_to": "habit_categories", "required": true},
    "subcategory_id": {"type": "link", "link_to": "habit_subcategories"},
    "icon": {"type": "text", "required": true},
    "type": {"type": "single_select", "options": ["good", "bad"], "required": true},
    "difficulty": {"type": "single_select", "options": ["easy", "medium", "hard"], "required": true},
    "points": {"type": "number", "required": true},
    "goal": {"type": "number", "required": true},
    "description": {"type": "text", "required": true},
    "tags": {"type": "long_text", "description": "JSON array of tags"},
    "tracking_type": {"type": "single_select", "options": ["simple", "quantity", "duration", "sets"]},
    "tracking_unit": {"type": "text"},
    "is_active": {"type": "boolean", "default": true},
    "usage_count": {"type": "number", "default": 0},
    "created_at": {"type": "date", "auto_now_add": true}
  },
