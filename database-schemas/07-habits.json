{
  "table_name": "habits",
  "description": "Core habits table - individual habit definitions - Import SEVENTH",
  "import_order": 7,
  "dependencies": ["habit_categories", "habit_subcategories", "habit_groups"],
  "fields": {
    "id": {"type": "text", "primary": true, "description": "Unique habit identifier"},
    "name": {"type": "text", "required": true, "description": "Habit name"},
    "type": {"type": "single_select", "options": ["good", "bad"], "required": true},
    "difficulty": {"type": "single_select", "options": ["easy", "medium", "hard"], "default": "medium"},
    "points": {"type": "number", "default": 10, "description": "Points awarded for completion"},
    "goal": {"type": "number", "description": "Target value (e.g., 30 minutes, 8 glasses)"},
    "reward": {"type": "text", "description": "Motivational reward text"},
    "category_id": {"type": "link", "link_to": "habit_categories"},
    "subcategory_id": {"type": "link", "link_to": "habit_subcategories"},
    "group_id": {"type": "link", "link_to": "habit_groups"},
    "tracking_type": {"type": "single_select", "options": ["simple", "quantity", "duration", "sets"], "default": "simple"},
    "tracking_unit": {"type": "text", "description": "Unit for tracking (minutes, glasses, reps, etc.)"},
    "target_value": {"type": "number", "description": "Target amount to track"},
    "icon": {"type": "text", "description": "Emoji or icon"},
    "description": {"type": "long_text"},
    "reminder_time": {"type": "text", "description": "Time for reminders (HH:MM)"},
    "frequency": {"type": "single_select", "options": ["daily", "weekly", "custom"], "default": "daily"},
    "target_days": {"type": "multiple_select", "options": ["monday", "tuesday", "wednesday", "thursday", "friday", "saturday", "sunday"]},
    "is_active": {"type": "boolean", "default": true},
    "user_id": {"type": "text", "description": "User who owns this habit"},
    "streak": {"type": "number", "default": 0, "description": "Current streak count"},
    "best_streak": {"type": "number", "default": 0, "description": "Best streak achieved"},
    "created_at": {"type": "date", "auto_now_add": true},
    "updated_at": {"type": "date", "auto_now": true}
  },
