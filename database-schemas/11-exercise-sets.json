{
  "table_name": "exercise_sets",
  "description": "Individual exercise sets within workout sessions - Import ELEVENTH",
  "import_order": 11,
  "dependencies": ["habit_entries", "exercise_details"],
  "fields": {
    "id": {"type": "auto_number", "primary": true},
    "habit_entry_id": {"type": "link", "link_to": "habit_entries", "required": true},
    "exercise_detail_id": {"type": "link", "link_to": "exercise_details", "required": true},
    "user_id": {"type": "text", "required": true},
    "set_number": {"type": "number", "required": true},
    "reps": {"type": "number"},
    "weight": {"type": "number", "description": "Weight in kg or lbs"},
    "weight_unit": {"type": "single_select", "options": ["kg", "lbs"], "default": "kg"},
    "duration": {"type": "number", "description": "Duration in seconds"},
    "distance": {"type": "number", "description": "Distance in meters/km"},
    "completed": {"type": "boolean", "default": false},
    "rest_time": {"type": "number", "description": "Rest time after set in seconds"},
    "notes": {"type": "text"},
    "difficulty_rating": {"type": "single_select", "options": ["easy", "moderate", "hard", "failure"], "description": "How difficult the set felt"},
    "form_rating": {"type": "single_select", "options": ["poor", "okay", "good", "excellent"], "description": "Self-assessment of form quality"},
    "created_at": {"type": "date", "auto_now_add": true}
  },
