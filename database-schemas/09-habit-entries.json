{
  "table_name": "habit_entries",
  "description": "Daily habit completion records - Import NINTH (depends on habits)",
  "import_order": 9,
  "dependencies": ["habits"],
  "fields": {
    "id": {"type": "auto_number", "primary": true},
    "habit_id": {"type": "link", "link_to": "habits", "required": true},
    "user_id": {"type": "text", "required": true},
    "date": {"type": "date", "required": true},
    "status": {"type": "single_select", "options": ["not-started", "in-progress", "completed", "skipped", "failed"], "default": "not-started"},
    "value": {"type": "number", "description": "Tracked value (duration, quantity, etc.)"},
    "notes": {"type": "long_text"},
    "mood": {"type": "single_select", "options": ["great", "good", "okay", "bad", "terrible"]},
    "time_spent": {"type": "number", "description": "Time spent in minutes"},
    "completed_at": {"type": "date_time"},
    "started_at": {"type": "date_time"},
    "reminder_sent": {"type": "boolean", "default": false},
    "points_earned": {"type": "number", "default": 0},
    "streak_day": {"type": "number", "description": "Which day of the streak this represents"},
    "created_at": {"type": "date", "auto_now_add": true},
    "updated_at": {"type": "date", "auto_now": true}
  },
