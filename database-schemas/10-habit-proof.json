{
  "table_name": "habit_proof",
  "description": "Proof documentation for habit completion - Import TENTH",
  "import_order": 10,
  "dependencies": ["habit_entries"],
  "fields": {
    "id": {"type": "auto_number", "primary": true},
    "habit_entry_id": {"type": "link", "link_to": "habit_entries", "required": true},
    "user_id": {"type": "text", "required": true},
    "proof_type": {"type": "single_select", "options": ["image", "text", "both"], "required": true},
    "image_url": {"type": "url", "description": "Link to uploaded proof image"},
    "image_caption": {"type": "text", "description": "Caption for the image"},
    "note": {"type": "long_text", "description": "Text proof/description"},
    "file_size": {"type": "number", "description": "Image file size in bytes"},
    "file_type": {"type": "text", "description": "Image MIME type"},
    "is_public": {"type": "boolean", "default": false, "description": "Whether proof can be shared"},
    "uploaded_at": {"type": "date_time", "auto_now_add": true},
    "created_at": {"type": "date", "auto_now_add": true}
  },
